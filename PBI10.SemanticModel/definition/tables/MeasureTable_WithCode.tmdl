	measure code_population_growth_pct = ```
			
			VAR CurrentYear = SELECTEDVALUE(population_code[Year])
			VAR CurrentPopulation = 
			    CALCULATE(
			        [code_population],
			        FILTER(
			            ALL(population_code),
			            population_code[Year] = CurrentYear
			            && population_code[Entity] = MAX(population_code[Entity])
			        )
			    )
			VAR PrevPopulation =
			    CALCULATE(
			        [code_population],
			        FILTER(
			            ALL(population_code),
			            population_code[Year] = CurrentYear - 1
			            && population_code[Entity] = MAX(population_code[Entity])
			        )
			    )
			RETURN
			IF(
			    NOT ISBLANK(PrevPopulation),
			    DIVIDE(CurrentPopulation - PrevPopulation, PrevPopulation),
			    BLANK()
			)
			```
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: b2aef943-69ae-461a-badc-3dde7eb7c340

	measure '// Annual Growth Rate (Corrected)Annual Growth Rate %' = ```
			
			VAR CurrentYearPopulation = CALCULATE(
			        SUM(population_code[Population (historical)]),
			        FILTER(
			            ALL(population_code),
			            population_code[Year] = MAX(population_code[Year])
			            && population_code[Entity] = MAX(population_code[Entity])
			        )
			    )
			VAR PreviousYearPopulation = 
			    CALCULATE(
			        SUM(population_code[Population (historical)]),
			        FILTER(
			            ALL(population_code),
			            population_code[Year] = MAX(population_code[Year]) - 1
			            && population_code[Entity] = MAX(population_code[Entity])
			        )
			    )
			RETURN
			DIVIDE(
			    CurrentYearPopulation - PreviousYearPopulation,
			    PreviousYearPopulation,
			    0
			)
			```
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: 299a6b79-e761-4a10-910e-8d98bce82957

	measure '// Create measure to show only first/last labelsSmart Label' =
			
			VAR CurrentYear = SELECTEDVALUE(population_no_code[Year])
			VAR FirstYear = MIN(population_no_code[Year])
			VAR LastYear = MAX(population_no_code[Year])
			RETURN
			IF(
			    OR(CurrentYear = FirstYear, CurrentYear = LastYear),
			    [code_population_growth_pct],
			    BLANK()
			)
		lineageTag: 425c7300-08ba-4511-b950-d5204b8b9a10


	measure code_population_YearColor =
			
			VAR CurrentYear = SELECTEDVALUE(population_code[Year])
			VAR MinY = CALCULATE(MIN(population_code[Year]), ALL(population_code))
			VAR MaxY = CALCULATE(MAX(population_code[Year]), ALL(population_code))
			RETURN
			SWITCH(TRUE(),
			    CurrentYear = MinY, "Blue",
			    CurrentYear = MaxY, "Blue",
			    "White"
			)
		lineageTag: cea64a68-3b4d-4df7-ba01-2f533b9b206e


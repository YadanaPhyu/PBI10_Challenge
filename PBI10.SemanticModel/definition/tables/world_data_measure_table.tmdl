	measure YearColor =
			
			VAR CurrentYear = SELECTEDVALUE(world_data_unpivot[Year])
			VAR MinY = CALCULATE(MIN(world_data_unpivot[Year]), ALL(world_data_unpivot))
			VAR MaxY = CALCULATE(MAX(world_data_unpivot[Year]), ALL(world_data_unpivot))
			RETURN
			SWITCH(TRUE(),
			    CurrentYear = MinY, "Blue",
			    CurrentYear = MaxY, "Blue",
			    "White"
			)
		lineageTag: c775b975-fd5f-4ad1-b52c-0596588377b1

	measure 'Page Filter' = ```
			
			VAR total_items = 
			CALCULATE(
			    DISTINCTCOUNT(world_data_unpivot[Entity]),
			    ALL(world_data_unpivot[Entity])
			)
			VAR show_number_items = [# Items Value]
			VAR _number_of_pages = 
			ROUNDUP(
			    DIVIDE(
			        total_items,
			        show_number_items
			    ),
			    0
			)
			VAR _page_filter = 
			IF(
			    SELECTEDVALUE('#Pages'[#Pages]) <= _number_of_pages,
			    1,
			    0
			)
			RETURN _page_filter
			```
		formatString: 0
		lineageTag: 9dcbe8f3-1023-44f1-991b-58f74c5dc126

